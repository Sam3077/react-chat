{
  "name": "react-chat",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "graphql": "^14.0.2",
    "graphql-request": "^1.8.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.0.17",
    "styled-components": "^3.4.9",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "scripts": {
    "start": "concurrently \"cd yoga-server && prisma deploy && node src/index.js\" \"npm run serve\"",
    "build": "react-scripts build",
    "develop": "react-scripts start",
    "install-dependencies": "npm install && cd yoga-server && npm install",
    "preserve": "npm run build",
    "serve": "serve -s build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "prisma": "^1.18.0",
    "serve": "^10.0.2"
  }
}
